@model Bfw.PXAP.Models.SettingsModel

@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
 }
<script src="@Url.Content("~/Scripts/PAPX/User.js")" type="text/javascript"></script>

<script type="text/javascript">
    $(document).ready(function () {
        PxApUser.BindGrid();

        $("#AddNewUser").click(function () {
            PxApUser.CreateUser();
        });
    });
</script>

<div style="width:400px; margin:0 auto;">
    @Html.Partial("_MainMenuPartial", Model.MenuModel)
</div>

<h2>Users</h2>
    
<div style="width:900px; margin:0 auto;">
    <div id="userMain">
        <table id="list" class="scroll" cellpadding="0" cellspacing="0">
        </table>
        <div id="pager" class="scroll" style="text-align: center;">
        </div>    
    </div>
    <div id="userInfo"></div>
</div>

<div style="width:150px; margin:0 auto;">
    <img id="AddNewUser" alt="Add New User" title="Add New User" src="/Content/images/add.png" style="cursor:pointer;" />
</div>




