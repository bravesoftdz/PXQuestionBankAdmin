	<item itemid="TMP_83a8a14d1cc34a54b84868257ac3f165" version="2" entityid="-entityid-" creationdate="2011-06-17T19:23:08.03Z" creationby="2" modifieddate="2012-01-25T17:16:20.89Z" modifiedby="7">
		<data>
		  <type>Resource</type>
		  <parent>PX_TEMPLATES</parent>
		  <sequence>f</sequence>
		  <title>Document Collection</title>
		  <description>
			&lt;br/&gt;
			A repository for useful files, typically
			those employed in the delivery of course
			information or material that offers educational
			support. The collection may be organized,
			referenced, or assigned by instructors for students
			to access
			&lt;br/&gt;
			&lt;br/&gt;
			Typical Uses
			&lt;br/&gt;
			• A location to host course information, policies,
			or processes.
			&lt;br/&gt;
			• A location to host documents related to
			activities or labs, articles, or worksheets to be
			used in class.
			&lt;br/&gt;
		  </description>
		  <folder>83a8a14d1cc34a54b84868257ac3f165</folder>
		  <href>Templates/Data/83a8a14d1cc34a54b84868257ac3f165/index.html</href>
		  <category>0</category>
		  <bfw_type>DocumentCollection</bfw_type>
		  <bfw_properties>
			<bfw_property name="bfw_visibility" type="String">&lt;bfw_visibility /&gt;</bfw_property>
			<bfw_property name="bfw_contentcreate_assign" type="string">true</bfw_property>
		  </bfw_properties>
		  <bfw_template>Document</bfw_template>
		</data>
	  </item>
