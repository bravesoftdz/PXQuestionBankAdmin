        <objective guid="c1749c55-6d3f-4f43-9391-aca64f01ad0d" id="10">
          <description>Practice appropriate means of documenting their work</description>
        </objective>
        <objective guid="a861c341-57e5-4978-ada6-c2f8127214b9" id="10">
          <description>Control such surface features as syntax, grammar, punctuation, and spelling</description>
        </objective>
        <objective guid="0ffcc622-6a85-4ba8-b03a-e54c8b9e527b" id="10">
          <description>Composing in Electronic Environments</description>
        </objective>
        <objective guid="46e0cd19-23dd-4540-84cc-7cc4fd5be07a" id="10">
          <description>Use electronic environments for drafting, reviewing, revising, editing, and sharing texts</description>
